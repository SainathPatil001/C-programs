#include <stdio.h>

int main()
 {
   int i,j,n;
   printf("enter the no:\n");
   scanf("%d",&n);
   for(i=1;i<=12;i++)
   {
     for(j=1;j<=30;j++)  
     {
      if((i==1 && (j==1 || j==6)) ||(i==2 && (j==1 || j== 5)) || (i==3 && (j==1 || j==4)) || (i==4 && (j==1 || j==3)) || (i==5 && (j==1 || j==2)) || (i==6 && (j==1 || j==1)) || (i==7 && (j==1 || j==2)) || (i==8 && (j==1 || j==3)) || (i==9 && (j==1 || j==4)) || (i==10 && (j==1 || j==5)) || (i==11 && (j==1 || j==6)))
      {
          printf("*");
      }
       else if(j==20-(i-1) || (j==20+(i-1)) || (i==6 &&( j>=15 && j<=25)))
        {
         printf("*");
        }
        else 
      {
          printf(" ");
      }
     }
     printf("\n");
   }
    return 0;
}

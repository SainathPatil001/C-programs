#include<stdio.h>
int main()
{
 int i,co=0,r;
 char arr[100],ch;
 printf("enter the string");
 fgets(arr,100,stdin);
 printf("enter a character whose frequency is to be known\n:");
 scanf("%c",&ch);
 for( i=0; arr[i] !='\0';++i)
 {
     if(ch == arr[i])
     {
         ++co;
     }
 }
 printf("frequency of %c is %d\n",ch,co);
 return 0;
}
